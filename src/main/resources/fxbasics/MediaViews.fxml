<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane prefHeight="400.0" prefWidth="461.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxbasics.MediaViewsController">
   <children>
      <MediaView fx:id="mediaView" fitHeight="300.0" fitWidth="500.0" layoutY="8.0" />
      <Button layoutX="81.0" layoutY="333.0" mnemonicParsing="false" onAction="#playMedia" text="Play" />
      <Button layoutX="224.0" layoutY="333.0" mnemonicParsing="false" onAction="#pauseMedia" text="Pause" />
      <Button layoutX="370.0" layoutY="333.0" mnemonicParsing="false" onAction="#resetMedia" text="Reset" />
   </children>
</AnchorPane>
